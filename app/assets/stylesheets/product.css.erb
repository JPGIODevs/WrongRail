.storemain{
  margin: 0;
  padding: 0;
}
.storemain-frame{
  position: absolute;
  left: 3vw;
  padding: 50px;
  background-color: rgba(255,255,255,0.5);
  backdrop-filter: blur(15px);
  border-radius: 15px;
  box-shadow: 0 0 20px 5px rgba(255,255,255,0.5) ;
}
.navbar{
  top: 2vh;
}
.productwrapper{
  padding: 5vw;
  display: grid;
  gap: 3vw;
  grid-template-columns: 1fr 1fr;
  grid-auto-rows: minmax(100px, auto);
}
.mainphoto{
  background: black;
  grid-column: 1 / 3;
  grid-row: 1/4;
}
