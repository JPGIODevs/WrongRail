.storemain{
  margin: 0;
  padding: 0;
}
.storemain-frame{
  left: 3vw;
  padding: 10px;
  background-color: rgba(255,255,255,0.5);
  backdrop-filter: blur(15px);
  border-radius: 15px;
  box-shadow: 0 0 20px 15px rgba(255,255,255,0.5) ;
  width: 35dvw;
  height: 80%;
  background-position: center center;
  background-repeat: no-repeat;
  overflow: hidden; 
}
.navbar{
  top: 2vh;
}
.productwrapper{
  padding: 2vh 5vw;
  display: flex;
  flex-direction: row;
}
.mainphoto{
  width: 35dvw;
  border-radius: 10px;
}
.largeframe{
}
.secondaryphotos{
  flex-grow: 2;
}
.secondaryphoto{
  width: 10dvw;
  padding: 10px;
  background-color: rgba(255,255,255,0.5);
  backdrop-filter: blur(15px);
  border-radius: 15px;
  box-shadow: 0 0 10px 5px rgba(255,255,255,0.5) ;
}
.formfield{
  width: 100%;
}
.shadowtext{
  text-shadow: 2px 2px 5px black;
}
.typecontainer{
  display: inline-block; 
}
.typewriter{
  display: inline-block;
  letter-spacing: 8px;
  margin: none;
  width: 100%;
  border-right: 5px solid grey;
  white-space: nowrap;
  overflow: hidden;
}
@keyframes cursor {
  50% { border-color: transparent }
}
@keyframes typing {
  from { width: 0 }
}
.productsection{
  background-color: rgba(255,255,255,0.7);
  box-shadow: inset 0 0 15px white;
  padding-top: 3vh;
}
.productheader{
  text-align: center;
  color: black;
}
.carousel .carousel-item {
  height: 66dvh;
  margin: 0 auto;
}
.carousel{
  margin-left: auto;
  margin-right: auto;
}
.carousel-item img {
  left: 0;
  min-height: 64dvh;
}
.carouselimage{
  position: absolute;
  top: -50%;
}
.productflexcontainer{
  display: flex;
  flex-direction: row;
  padding: 2vw;
  gap: 1vw;
}
.flexitem{
  width: 50%;
  height: 60dvh;
  text-align: center;
}
.flexfirst{
  background: url(<%= asset_path "productresources/outdoor.png" %>) no-repeat center;
  background-size: cover; 
}
.flexsecond{
  background: url(<%= asset_path "productresources/active.JPG" %>) no-repeat center;
  background-size: cover;   
}
.landingheader{
  position: relative;
  top: 40%;
}
.explorebtn{
  font-weight: bold;
}
